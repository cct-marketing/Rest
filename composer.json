{
    "name": "cct-marketing/rest",
    "description": "Base library to create an API sdk for CCT Microservices",
    "type": "library",
    "require": {
        "php": "~7.1",
        "guzzlehttp/guzzle": "^6.2",
        "symfony/yaml": "^4.0",
        "symfony/http-foundation": "^4.0",
        "beberlei/assert": "^2.8"
    },
    "require-dev": {
        "phpunit/phpunit": "^6.5",
        "jms/serializer": "^1.11",
        "symfony/serializer": "^4.0",
        "symfony/property-access": "^4.0"
    },
    "autoload": {
        "psr-4": {
            "CCT\\Component\\Rest\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "CCT\\Component\\Rest\\Tests\\": "tests/"
        }
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Jorge Vahldick",
            "email": "jorge.dev@outlook.com"
        },
        {
            "name": "Paul Schweppe",
            "email": "ps@cct-marketing.com"
        }
    ],
    "config": {
        "bin-dir": "bin"
    },
    "minimum-stability": "stable"
}
