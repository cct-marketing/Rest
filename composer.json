{
    "name": "cct-marketing/rest",
    "description": "Base library to create an API sdk for CCT Microservices",
    "type": "library",
    "require": {
        "php": "~7.1 | ~8.0",
        "guzzlehttp/guzzle": "~7.0",
        "symfony/yaml": "~4.4|~5.0|~6.0",
        "symfony/http-foundation": "~4.4|~5.0|~6.0",
        "cct-marketing/collections": "~1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.0",
        "jms/serializer": "^2.3|^3.28",
        "symfony/serializer": "^4.0|~5.0|~6.0",
        "symfony/property-access": "~4.0|~5.0|~6.0"
    },
    "autoload": {
        "psr-4": {
            "CCT\\Component\\Rest\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "CCT\\Component\\Rest\\Tests\\": "tests/"
        }
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Jorge Vahldick",
            "email": "jorge.dev@outlook.com"
        },
        {
            "name": "Paul Schweppe",
            "email": "ps@cct-marketing.com"
        }
    ],
    "config": {
        "bin-dir": "bin"
    },
    "minimum-stability": "stable"
}
